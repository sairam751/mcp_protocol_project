{
    "mcpServers": {
      "playwright": {
        "command": "npx",
        "args": ["@playwright/mcp@latest"],
        "env": {
          "DISPLAY": ":1"
        }
      },
      "airbnb": {
        "command": "npx",
        "args": [
          "-y",
          "@openbnb/mcp-server-airbnb"
        ]
      },
      "duckduckgo-search": {
        "command": "npx",
        "args": [
          "-y",
          "duckduckgo-mcp-server"
        ]
    },
     "weather": {
      "command": "uv",
      "args": [
        "run",
        "--with",
        "mcp[cli]",
        "mcp",
        "run",
        "L:/Pdfs/ReposGitHub/Modern_Contextprotocol_Client_Server/Server/weather.py"
      ]
    }
    }
  }